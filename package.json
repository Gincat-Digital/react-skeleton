  "author": "Braian Rosas <brdevok@outlook.com> (https://github.com/brdevok)",
  "license": "MIT",
  "scripts": {
    "dev": "set NODE_OPTIONS=--experimental-specifier-resolution=node --no-warnings --loader=ts-node/esm && gcwp --config .webpack/webpack.dev.ts",
    "build": "set NODE_OPTIONS=--experimental-specifier-resolution=node --no-warnings --loader=ts-node/esm && rm -rf dist && gcwp --config .webpack/webpack.prod.ts",
    "plop": "node --experimental-specifier-resolution=node --no-warnings --loader=ts-node/esm .scripts/plop.ts",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "type": "module",
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.1",
    "@gincat-digital/eslint-config-base": "^0.0.1",
    "@gincat-digital/webpack": "^0.0.1-alpha.1",
    "@storybook/addon-essentials": "^8.0.6",
    "@storybook/addon-interactions": "^8.0.6",
    "@storybook/addon-links": "^8.0.6",
    "@storybook/addon-onboarding": "^8.0.6",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
    "@storybook/blocks": "^8.0.6",
    "@storybook/react": "^8.0.6",
    "@storybook/react-webpack5": "^8.0.6",
    "@storybook/test": "^8.0.6",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.23",
    "eslint-plugin-storybook": "^0.8.0",
    "msw": "^1.3.3",
    "msw-storybook-addon": "^1.10.0",
    "plop": "^4.0.1",
    "prettier": "^3.2.5",
    "storybook": "^8.0.6",
    "storybook-react-i18next": "^3.0.1",
    "typescript": "^5.4.3",
    "webpack": "^5.91.0"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "clsx": "^2.1.0",
    "i18next": "^23.10.1",
    "i18next-http-backend": "^2.5.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.22.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2"
  },
  "msw": {
    "workerDirectory": ".msw\\worker"
  }
